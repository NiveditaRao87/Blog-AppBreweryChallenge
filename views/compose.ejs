<%- include("partials/header"); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" name="postTitle" class="form-control" autofocus autocomplete="off">
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea class="form-control" name="postContent"  rows="4"></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="publish">Publish</button>
</form>
<%- include("partials/footer"); -%>
